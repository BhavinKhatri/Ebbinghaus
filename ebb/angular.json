{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "version": 1,
    "newProjectRoot": "projects",
    "projects": {
      "ManageTaxRemittanceAccount": {
        "projectType": "application",
        "schematics": {
          "@schematics/angular:component": {
            "style": "less"
          },
          "@schematics/angular:application": {
            "strict": true
          }
        },
        "root": "",
        "sourceRoot": "src",
        "prefix": "app",
        "architect": {
          "build": {
            "builder": "@angular-builders/custom-webpack:browser",
            "options": {
              "customWebpackConfig": {
                "path": "./custom-webpack.config.ts",
                "replaceDuplicatePlugins": true
              },
              "progress": true,
              "outputPath": "wwwroot/dist/",
              "resourcesOutputPath": "fonts",
              "deleteOutputPath": true,
              "index": "src/index.html",
              "main": "src/main.ts",
              "polyfills": "Client/Scripts/dependencies.ts",
              "tsConfig": "tsconfig.app.json",
              "inlineStyleLanguage": "less",
              "aot": true,
              "assets": [
                {
                  "glob": "app.config.json",
                  "input": "./",
                  "output": "./"
                },
                {
                  "glob": "configHelper.js",
                  "input": "Client/Scripts/",
                  "output": "./"
                },
                {
                  "glob": "**/*",
                  "input": "node_modules/tyler-ui-omnibar/dist/",
                  "output": "./"
                },
                {
                  "glob": "ng-flow-standalone.min.js",
                  "input": "node_modules/ng-flow/dist/",
                  "output": "./"
                },
                {
                  "glob": "angular-q-extras.min.js",
                  "input": "node_modules/angular-q-extras/dist/",
                  "output": "./"
                },
                {
                  "glob": "**/*",
                  "input": "node_modules/angular-esri-map/dist/",
                  "output": "./"
                },
                {
                  "glob": "**/*",
                  "input": "node_modules/energov-attachments-ui/dist/fonts/",
                  "output": "fonts"
                }
              ],
              "styles": [
                {
                  "input": "src/styles.less",
                  "bundleName": "dependencies"
                }
              ],
              "scripts": [],
              "allowedCommonJsDependencies": [
                "gettext-parser"
              ]
            },
            "configurations": {
              "release": {
                "optimization": true,
                "sourceMap": false,
                "namedChunks": true,
                "outputHashing": "all",
                "aot": true,
                "extractLicenses": true,
                "vendorChunk": false,
                "buildOptimizer": true,
                "verbose": false
              },
              "development": {
                "optimization": false,
                "sourceMap": true,
                "namedChunks": true,
                "outputHashing": "none",
                "aot": true,
                "extractLicenses": false,
                "vendorChunk": false,
                "buildOptimizer": false,
                "verbose": false
              }
            },
            "defaultConfiguration": "release"
          },
          "serve": {
            "builder": "@angular-builders/custom-webpack:dev-server",
            "configurations": {
              "release": {
                "browserTarget": "ManageTaxRemittanceAccount:build:release"
              },
              "development": {
                "browserTarget": "ManageTaxRemittanceAccount:build:development"
              }
            },
            "defaultConfiguration": "development"
          },
          "extract-i18n": {
            "builder": "@angular-builders/custom-webpack:extract-i18n",
            "options": {
              "browserTarget": "ManageTaxRemittanceAccount:build"
            }
          },
          "test": {
            "builder": "@angular-builders/custom-webpack:karma",
            "options": {
              "main": "src/test.ts",
              "polyfills": "src/polyfills.ts",
              "tsConfig": "tsconfig.spec.json",
              "karmaConfig": "karma.conf.js",
              "inlineStyleLanguage": "less",
              "assets": [
                "src/favicon.ico",
                "src/assets"
              ],
              "styles": [
                "src/styles.less"
              ],
              "scripts": []
            }
          }
        }
      }
    }
  }